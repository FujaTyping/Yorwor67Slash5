{
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
        }
    ],
    "buildCommand": "bun install",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "functions": {
        "src/api/**/*.js": {
            "includeFiles": "node_modules/@sparticuz/chromium/**"
        }
    },
    "git": {
        "deploymentEnabled": {
            "main": false,
            "dev": false,
            "api": false
        }
    }
}